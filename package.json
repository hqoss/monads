{
  "name": "@sniptt/monads",
  "version": "0.5.10",
  "description": "Option, Result, and Either types for JavaScript; Inspired by Rust",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=14",
    "npm": ">=7"
  },
  "keywords": [
    "monads",
    "option",
    "result",
    "either",
    "javascript",
    "js",
    "typescript",
    "node",
    "node.js"
  ],
  "author": "Sniptt <support@sniptt.com> (sniptt.com)",
  "license": "MIT",
  "homepage": "https://github.com/sniptt-official/monads",
  "bugs": "https://github.com/sniptt-official/monads/issues",
  "scripts": {
    "build": "rollup -c",
    "docs": "typedoc --out docs src/index.ts --excludeNotDocumented",
    "format": "prettier --check '**/*.{ts,mjs,json}' '.github/**/*.{yml,yaml}'",
    "format:fix": "prettier --write '**/*.{ts,mjs,json}' '.github/**/*.{yml,yaml}'",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "test": "jest",
    "test:cover": "jest --coverage --verbose --runInBand"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "@thames/eslint-config": "^0.0.4",
    "@thames/prettier-config": "^0.0.4",
    "@tsconfig/node20": "^20.1.4",
    "@types/jest": "^29.5.12",
    "jest": "^29.7.0",
    "prettier": "3.2.5",
    "rollup": "^4.13.2",
    "ts-jest": "^29.1.2",
    "typedoc": "^0.25.12",
    "typescript": "^5.4.3"
  }
}
